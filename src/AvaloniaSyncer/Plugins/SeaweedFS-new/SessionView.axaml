<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:AvaloniaSyncer.Plugins.SeaweedFS_new"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaSyncer.Plugins.SeaweedFS_new.SessionView"
             x:DataType="local:SessionViewModel">

	<StackPanel>
		<ContentControl Content="{Binding Configuration}" />
		<HeaderedContentControl Header="Path">
			<TextBox Watermark="Path" Text="{Binding Path}" />
		</HeaderedContentControl>
	</StackPanel>
</UserControl>
