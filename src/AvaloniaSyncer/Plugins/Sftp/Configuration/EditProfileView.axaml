<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaSyncer.Plugins.Sftp.Configuration.EditProfileView"
             x:Name="UserControl"
             >
    <StackPanel>
        <TextBox Watermark="Name" Text="{Binding #UserControl.Profile.Name}" />
        <TextBox Watermark="Host" Text="{Binding #UserControl.Profile.Host}" />
        <TextBox Watermark="Port" Text="{Binding #UserControl.Profile.Port}" />
        <TextBox Watermark="Username" Text="{Binding #UserControl.Profile.Username}" />
        <TextBox Watermark="Password" Text="{Binding #UserControl.Profile.Password}" />
        <Button Content="{Binding #UserControl.CommandText}" Command="{Binding #UserControl.Command}" />
    </StackPanel>
</UserControl>
